{
    "name": "language-lingvodsl",
    "displayName": "Lingvo DSL",
    "description": "Syntax support for Lingvo DSL files",
    "publisher": "quendimax",
    "repository": {
        "type": "git",
        "url": "https://github.com/quendimax/language-lignvodsl"
    },
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "lingvodsl",
            "aliases": ["Lingvo DSL", "lingvodsl"],
            "extensions": [".dsl"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lingvodsl",
            "scopeName": "text.dsl",
            "path": "./syntaxes/lingvodsl.tmLanguage.json"
        }]
    }
}